    "@testing-library/user-event": "^13.5.0",
    "aws-amplify": "^5.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-scripts": "5.0.1"
